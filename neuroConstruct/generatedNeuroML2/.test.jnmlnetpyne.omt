# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_TestNet.xml
engine: jNeuroML_NetPyNE
mep: .test.mep
experiments:
  gran:
    observables:
      spike times:
        file:
          path: SingleGranule_0.0.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: 0
        tolerance: 0.0
      
